<?xml version='1.0' encoding='utf-8'?>
<widget id="edu.rutgers.CATBox" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="SpinnerDialog">
        <param name="ios-package" value="CDVSpinnerDialog" />
    </feature>
    <splash height="768" src="image_resources/Launch_Landscape_1024x768.png" width="1024" />
    <splash height="1536" src="image_resources/Launch_Landscape_1536x2045.png" width="2045" />
    <splash height="1024" src="image_resources/Launch_Portrait_1024x768.png" width="768" />
    <splash height="2045" src="image_resources/Launch_Portrait_1536x2045.png" width="1536" />
    <icon height="1024" src="image_resources/iTunesArtwork@2x.png" width="1024" />
    <icon height="152" src="image_resources/Icon-76@2x.png" width="152" />
    <icon height="29" src="image_resources/Icon-Small.png" width="29" />
    <icon height="40" src="image_resources/Icon-40.png" width="40" />
    <icon height="512" src="image_resources/iTunesArtwork.png" width="512" />
    <icon height="58" src="image_resources/Icon-Small@2x.png" width="58" />
    <icon height="80" src="image_resources/Icon-40@2x.png" width="80" />
    <icon height="120" src="image_resources/Icon-60@2x.png" width="120" />
    <icon height="180" src="image_resources/Icon-60@3x.png" width="180" />
    <icon height="76" src="image_resources/Icon-76.png" width="76" />
    <icon height="87" src="image_resources/Icon-Small@3x.png" width="87" />
    <name>CATBox</name>
    <description>
        Graph animation software.
    </description>
    <author email="sbmerkling@gmail.com" href="bioinformatics.rutgers.edu">
        Alexander Schliep, Scott Merkling, Matthew Mitsui.
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="Orientation" value="default" />
    <preference name="orientation" value="default" />
    <preference name="DisallowOverscroll" value="true" />
    <gap:plugin name="hu.dpal.phonegap.plugins.spinnerdialog" />
</widget>
