<?xml version='1.0' encoding='utf-8'?>
<widget id="edu.rutgers.gato" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Gato</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="Orientation" value="landscape" />
    <preference name="orientation" value="landscape" />
    <preference name="DisallowOverscroll" value="true"/>
    <gap:plugin name="it.mobimentum.phonegapspinnerplugin" />
    <gap:plugin name="hu.dpal.phonegap.plugins.spinnerdialog" />
</widget>
